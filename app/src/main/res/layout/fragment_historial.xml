                android:ems="10"
                android:inputType="text"
                android:text="Name" />

            <View
                android:id="@+id/divider4"
                android:layout_width="match_parent"
                android:layout_height="31dp"
                android:background="?android:attr/listDivider" />

            <TextView
                android:id="@+id/txtMensaje5"
