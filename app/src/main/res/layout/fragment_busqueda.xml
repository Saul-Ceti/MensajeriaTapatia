<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment_busqueda">

    <TextView
        android:id="@+id/txtTitulo4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/menu_busqueda"
        tools:layout_editor_absoluteX="178dp"
        tools:layout_editor_absoluteY="47dp" />

    <TextView
        android:id="@+id/txtinfoBusqueda"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="71dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="72dp"
        android:text="@string/infoBusqueda"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtTitulo4" />

    <EditText
        android:id="@+id/edtBuscar"
        android:layout_width="347dp"
        android:layout_height="64dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="33dp"
        android:layout_marginEnd="29dp"
        android:ems="10"
        android:hint="@string/folio"
        android:inputType="text"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtinfoBusqueda" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="339dp"
        android:layout_height="330dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="66dp"
        android:layout_marginEnd="38dp"
        android:layout_marginBottom="127dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edtBuscar" />
</androidx.constraintlayout.widget.ConstraintLayout>